{
  "name": "mindconnect-backend",
  "version": "1.0.0",
  "description": "MindConnect Mental Health Platform Backend with Gemini AI, Auth0, and Google Calendar",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "reset-db": "node scripts/reset-db.js",
    "setup": "npm run migrate && npm run seed",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.js\"",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "socket.io": "^4.6.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "express-validator": "^7.0.1",
    "axios": "^1.6.2",
    "nodemailer": "^6.9.7",
    "winston": "^3.11.0",
    "@google/generative-ai": "^0.1.3",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "auth0": "^4.2.0",
    "twilio": "^4.19.0",
    "googleapis": "^128.0.0",
    "multer": "^1.4.5-lts.1",
    "date-fns": "^3.0.6",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "mental-health",
    "therapy",
    "ai-assistant",
    "socket.io",
    "auth0",
    "gemini",
    "twilio",
    "google-calendar"
  ],
  "author": "MindConnect Team",
  "license": "MIT"
}

